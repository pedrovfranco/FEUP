package circuits;

import java.util.List;

public class CombinatorialCircuit
{
	List<LogicVariable> variables;
	
	public boolean addVariable(LogicVariable a)
	{
		if (a == null)
			return false;
		
		if (variables.contains(a))
			return false;
		else
		{
			variables.add(a);
			return true;
		}
	}
	
