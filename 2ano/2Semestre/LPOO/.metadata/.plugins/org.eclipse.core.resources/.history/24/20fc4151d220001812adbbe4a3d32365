package building;

public class Room
{
	private Building building;
	private String number;
	private int floor;
	
	public Room(String name, int minFloor, int maxFloor) throws IllegalArgumentException
	{
		if (name == null)
			throw new IllegalArgumentException();
		
		if (maxFloor < minFloor)
			throw new IllegalArgumentException();
		
		this.name = name;
		this.minFloor = minFloor;
		this.maxFloor = maxFloor;
	}
}
