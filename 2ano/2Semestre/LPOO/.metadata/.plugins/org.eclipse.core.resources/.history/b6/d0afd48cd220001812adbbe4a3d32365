	private String number;
	private int floor;
	
	public Room(Building building, String number, int floor) throws IllegalArgumentException
	{
		if (b == null)
			throw new IllegalArgumentException();
		
		if (maxFloor < number)
			throw new IllegalArgumentException();
		
